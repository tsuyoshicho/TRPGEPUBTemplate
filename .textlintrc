{
  "plugins": {
    "@textlint/markdown": {
      "extensions": [".markdown", ".mdown", ".mkdn", ".mkd", ".md"]
    }
  },
  "rules": {
    "@proofdict/proofdict": false,
    "preset-japanese": {
      "sentence-length": false,
      "max-ten": {
        "max": 5
      },
      "no-mix-dearu-desumasu": false
    }
  }
}
